<p id="notice"><%= notice %></p>

<h1>Feedback Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Feedback</th>
      <th>Sender</th>
      <th>Receiver</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedback_assignments.each do |feedback_assignment| %>
      <tr>
        <td><%= feedback_assignment.feedback_id %></td>
        <td><%= feedback_assignment.sender_id %></td>
        <td><%= feedback_assignment.receiver_id %></td>
        <td><%= link_to 'Show', feedback_assignment %></td>
        <td><%= link_to 'Edit', edit_feedback_assignment_path(feedback_assignment) %></td>
        <td><%= link_to 'Destroy', feedback_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback Assignment', new_feedback_assignment_path %>
